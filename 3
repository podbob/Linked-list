class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

def has_cycle(head):
    slow = head
    fast = head

    while fast is not None and fast.next is not None:
        slow = slow.next
        fast = fast.next.next

        if slow == fast:
            return True

    return False

# Приклад використання:
# Створення циклічного списку
head1 = ListNode(3)
node1 = ListNode(2)
node2 = ListNode(0)
node3 = ListNode(-4)

head1.next = node1
node1.next = node2
node2.next = node3
node3.next = node1  

result1 = has_cycle(head1)
print(result1)  # Виведе: True

head2 = ListNode(1)
node4 = ListNode(2)

head2.next = node4

result2 = has_cycle(head2)
print(result2)  # Виведе: False
