class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

def delete_duplicates(head):
    current = head

    while current is not None and current.next is not None:
        if current.val == current.next.val:
            current.next = current.next.next
        else:
            current = current.next

    return head

# Функція для виведення списку
def print_list(head):
    current = head
    while current is not None:
        print(current.val, end=" ")
        current = current.next
    print("None")

# Приклад використання:
head1 = ListNode(1, ListNode(1, ListNode(2)))
head2 = ListNode(1, ListNode(1, ListNode(2, ListNode(3, ListNode(3)))))

result1 = delete_duplicates(head1)
result2 = delete_duplicates(head2)

print_list(result1)  # Виведе: 1 -> 2 -> None
print_list(result2)  # Виведе: 1 -> 2 -> 3 -> None
